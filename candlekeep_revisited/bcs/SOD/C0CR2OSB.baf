// DYNAHEIR

IF
	InMyArea(Player1)
	!ActuallyInCombat()
	!GlobalTimerNotExpired("BD_BANTER_DELAY","MYAREA")
	GlobalGT("C02OspreyDynaheir1","MYAREA",0)
